{
  "name": "api-foundation",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "dev": "DEBUG=api-articles* nodemon -r tsconfig-paths/register src/index.ts ",
    "start": "DEBUG=api-articles* node dist/index.js",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests",
    "test:unit": "npm test -- --watchAll -c jest-unit-config.js",
    "test:integration": "npm test -- --watchAll -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "format:check": "prettier --check src",
    "format:write": "prettier --write src",
    "lint:check": "eslint -c .eslintrc --ext .ts src",
    "lint:fix": "eslint -c .eslintrc --fix --ext .ts src",
    "prepare": "husky install"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@shelf/jest-mongodb": "^4.0.0",
    "body-parser": "^1.20.0",
    "dotenv": "^16.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "^4.18.1",
    "git-commit-msg-linter": "^4.1.3",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "mongodb": "^4.8.1",
    "nodemon": "^2.0.19",
    "reflect-metadata": "^0.1.13",
    "swagger-jsdoc": "^6.2.4",
    "swagger-ui-express": "^4.5.0",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "tscpaths": "0.0.9",
    "tsyringe": "^4.7.0",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^18.6.3",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "prettier": "^2.7.1",
    "tsconfig-paths": "^4.1.0"
  }
}
